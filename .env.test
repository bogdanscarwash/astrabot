# Test environment variables
TESTING=true
LOG_LEVEL=DEBUG
YOUR_RECIPIENT_ID=2

# Mock API keys for testing (not real keys)
OPENAI_API_KEY=test_openai_key_1234567890
ANTHROPIC_API_KEY=test_anthropic_key_1234567890
HF_TOKEN=test_hf_token_1234567890

# Test-specific settings
NITTER_INSTANCES=nitter.test,nitter.example.com
CACHE_TTL_SECONDS=60
ENABLE_CACHING=true

# Model settings for testing
