# Example environment variables for Astrabot
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control!

# API Keys
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Signal Backup Settings (if needed)
SIGNAL_BACKUP_PATH=/path/to/signal/backup
SIGNAL_BACKUP_PASSWORD=your-backup-password

# Model Configuration
ANTHROPIC_MODEL=claude-3-opus-20240229
OPENAI_MODEL=gpt-4o-mini

# Development Settings
DEBUG=false
LOG_LEVEL=INFO

# Paths
DATA_DIR=/home/percy/git/astrabot/data
OUTPUT_DIR=/home/percy/git/astrabot/output

# Feature Flags
ENABLE_IMAGE_PROCESSING=true
ENABLE_BATCH_PROCESSING=true
MAX_BATCH_SIZE=10

# Rate Limiting
API_RATE_LIMIT=60
API_RATE_WINDOW=60

# Your recipient ID for Signal data processing
YOUR_RECIPIENT_ID=2